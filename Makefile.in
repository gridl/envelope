.PHONY: test test-ssl lldb lldb-ssl gdb gdb-ssl valgrind valgrind-ssl destroy

all:
	$(MAKE) -C envelope all

test-common:
	$(MAKE) -C envelope test-common

test:
	$(MAKE) -C envelope test

test-ssl:
	$(MAKE) -C postage test-ssl

lldb:
	$(MAKE) -C envelope lldb

lldb-ssl:
	$(MAKE) -C postage lldb-ssl

gdb:
	$(MAKE) -C envelope gdb

gdb-ssl:
	$(MAKE) -C postage gdb-ssl

valgrind:
	$(MAKE) -C envelope valgrind

valgrind-ssl:
	$(MAKE) -C postage valgrind-ssl

install:
	$(MAKE) -C envelope install

uninstall:
	$(MAKE) -C envelope uninstall

destroy:
	$(MAKE) -C envelope destroy

clean:
	$(MAKE) -C envelope clean
